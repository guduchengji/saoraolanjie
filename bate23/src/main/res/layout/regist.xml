<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".activity.RegistActivity"
                                                   tools:layout_editor_absoluteY="81dp">


    <TextView
            android:id="@+id/textView3"
            android:layout_width="0dp"
            android:layout_height="25dp"
            android:layout_marginStart="67dp"
            android:layout_marginEnd="7dp"
            android:text="账  号："
            app:layout_constraintBaseline_toBaselineOf="@+id/registusername"
            app:layout_constraintEnd_toStartOf="@+id/registusername"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="67dp"
            android:layout_marginRight="7dp"/>

    <TextView
            android:id="@+id/textView4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="67dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="7dp"
            android:layout_marginBottom="7dp"
            android:text="密  码："
            app:layout_constraintBottom_toBottomOf="@+id/registpassword"
            app:layout_constraintEnd_toStartOf="@+id/registpassword"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/registpassword" android:layout_marginLeft="67dp"
            android:layout_marginRight="7dp"/>

    <EditText
            android:id="@+id/registusername"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="129dp"
            android:layout_marginEnd="41dp"
            android:ems="10"
            android:inputType="textMultiLine"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginRight="41dp"/>

    <EditText
            android:id="@+id/registpassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="17dp"
            android:layout_marginEnd="41dp"
            android:ems="10"
            android:inputType="textPassword"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/registusername" android:layout_marginRight="41dp"/>

    <Button
            android:id="@+id/registregist"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="53dp"
            android:text="注  册"
            app:layout_constraintBaseline_toBaselineOf="@+id/registback"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/registback" android:layout_marginRight="53dp"/>

    <Button
            android:id="@+id/registback"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="55dp"
            android:layout_marginTop="37dp"
            android:layout_marginEnd="26dp"
            android:text="返  回"
            app:layout_constraintEnd_toStartOf="@+id/registregist"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/registpassword" android:layout_marginLeft="55dp"
            android:layout_marginRight="26dp"/>

    <MultiAutoCompleteTextView
            android:id="@+id/multiAutoCompleteTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="20dp"
            android:text="注   册   中   心"
            app:layout_constraintBottom_toTopOf="@+id/registusername"
            app:layout_constraintStart_toEndOf="@+id/textView3" android:layout_marginLeft="20dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>